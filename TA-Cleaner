   "source": [
    "def remove_before_period(s):\n",
    "    index = s.find('.')\n",
    "    if index != -1:\n",
    "        return s[index+1:]\n",
    "    else:\n",
    "        return s\n",
    "df['name'] = df['name'].apply(remove_before_period)"
   ]
