    "def split_string(s):\n",
    "    last_rupee_index = s.rfind('₹')  \n",
    "    if last_rupee_index != -1:\n",
    "        cuisines = s[last_rupee_index + 4:]\n",
    "        currency = s[:last_rupee_index]\n",
    "        return currency.strip(), cuisines.strip()\n",
    "    else:\n",
    "        return '', s\n",
    "df[['currency', 'cuisines']] = df['Pricing/Cuisine'].apply(split_string).apply(pd.Series)\n",
    "df"
   ]
